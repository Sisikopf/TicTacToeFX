<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import java.net.URL?>
<GridPane fx:controller="sample.Controller.Controller" fx:id="gridPane"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10" >

    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>

    <Button GridPane.columnIndex="1" GridPane.rowIndex="1" onAction="#buttonClick" prefWidth="50"
            prefHeight="50"></Button>
    <Button GridPane.columnIndex="2" GridPane.rowIndex="1" onAction="#buttonClick" prefWidth="50"
            prefHeight="50"></Button>
    <Button GridPane.columnIndex="3" GridPane.rowIndex="1" onAction="#buttonClick" prefWidth="50"
            prefHeight="50"></Button>
    <Button GridPane.columnIndex="1" GridPane.rowIndex="2" onAction="#buttonClick" prefWidth="50"
            prefHeight="50"></Button>
    <Button GridPane.columnIndex="2" GridPane.rowIndex="2" onAction="#buttonClick" prefWidth="50"
            prefHeight="50"></Button>
    <Button GridPane.columnIndex="3" GridPane.rowIndex="2" onAction="#buttonClick" prefWidth="50"
            prefHeight="50"></Button>
    <Button GridPane.columnIndex="1" GridPane.rowIndex="3" onAction="#buttonClick" prefWidth="50"
            prefHeight="50"></Button>
    <Button GridPane.columnIndex="2" GridPane.rowIndex="3" onAction="#buttonClick" prefWidth="50"
            prefHeight="50"></Button>
    <Button GridPane.columnIndex="3" GridPane.rowIndex="3" onAction="#buttonClick" prefWidth="50"
            prefHeight="50"></Button>
</GridPane>
